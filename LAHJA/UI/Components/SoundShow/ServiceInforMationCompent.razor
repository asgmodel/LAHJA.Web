                </MudPaper>

                <MudText Typo="Typo.h6" Class="mt-3">Model Tree</MudText>
                <MudExpansionPanels>
                    @foreach (var item in Param.ModelTree)
                    {
                        <MudExpansionPanel Text="@item.Title">
                            <MudText Typo="Typo.body2">@item.Description</MudText>
                        </MudExpansionPanel>
                    }
