@using LAHJA.UI.Components.ProFile.Charts
@using LAHJA.UI.Components.Statistics
@using Microsoft.AspNetCore.Authorization
@using System.Globalization
@attribute [Authorize]
@inject IManageLanguageService _manageLanguageService





<IframeComponent Lg="@CultureInfo.CurrentUICulture.Name"
    StyleCss="width:100%;height:700px !important;border:none;"
    IsLoading="true" 
    ServiceId="serv_5c095af3909547e888f2a0636a307ff9"
    BackupPageUrl="https://modelspeech.onrender.com/dashboard/" />


  

@code {

    [Parameter]
    public string Theme {get; set; }

    private string lg="ar";

    protected override async void OnAfterRender(bool firstRender)
    {

        if (firstRender)
        {
            lg = await _manageLanguageService.GetLanguageAsync();
            StateHasChanged();

        }


    }

}
